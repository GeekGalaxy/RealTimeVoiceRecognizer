<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">
    <sys:String x:Key="MinimizeButtonToolTip">Minimize application</sys:String>
    <sys:String x:Key="AboutButtonToolTip">Read about application</sys:String>
    <sys:String x:Key="CloseButtonToolTip">Close application</sys:String>
    <sys:String x:Key="MadeByLabel">Copyright © Anton Yarkov 2011</sys:String>
    <sys:String x:Key="FileDialogSettings">Text file|*.txt</sys:String>
